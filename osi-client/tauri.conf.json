{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "./manager/interface",
    "distDir": "./manager/interface",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "osi",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "osi",
      "icon": [
        "manager/icons/32x32.png",
        "manager/icons/128x128.png",
        "manager/icons/128x128@2x.png",
        "manager/icons/icon.icns",
        "manager/icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "OSI Manager",
        "width": 800,
        "height": 600
      }
    ]
  }
}